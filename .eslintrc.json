{
  "extends": [
    "semistandard",
    "standard-jsx",
    "prettier",
    "prettier/standard",
    "plugin:import/errors"
  ],
  "rules": {
    "curly": ["error", "multi-line"],
    "space-before-function-paren": ["error", "always"],
    "import/no-unresolved": ["error", { "commonjs": true }]
  }
}
